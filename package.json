{
  "name": "derealize",
  "productName": "Derealize",
  "description": "Tailwindcss for Designer",
  "scripts": {
    "start": "node -r @babel/register ./.erb/scripts/CheckPortInUse.js && webpack serve --config ./.erb/configs/webpack.babel.js",
    "start:with_runtime": "node -r @babel/register ./.erb/scripts/CheckPortInUse.js && cross-env WITH_RUNTIME=true webpack serve --config ./.erb/configs/webpack.babel.js",
    "start:preload": "webpack --config ./.erb/configs/webpack.dev.preload.babel.js",
    "start:main": "electron -r ./.erb/scripts/BabelRegister ./src/main.ts",
    "start:main:ext": "cross-env UPGRADE_EXTENSIONS=true electron -r ./.erb/scripts/BabelRegister ./src/main.ts",
    "start:backend": "webpack --config ./.erb/configs/webpack.dev.backend.babel.js",
    "build": "concurrently \"yarn build:preload\" \"yarn build:main\" \"yarn build:renderer\"",
    "build:with_runtime": "cross-env WITH_RUNTIME=true concurrently \"yarn build:preload\" \"yarn build:main\" \"yarn build:renderer\"",
    "build:preload": "webpack --config ./.erb/configs/webpack.prod.preload.babel.js",
    "build:main": "webpack --config ./.erb/configs/webpack.prod.main.babel.js",
    "build:renderer": "webpack --config ./.erb/configs/webpack.prod.babel.js",
    "package": "shx rm -rf src/dist && yarn build && electron-builder build --publish never",
    "package:with_runtime": "shx rm -rf src/dist && yarn build:with_runtime && cross-env WITH_RUNTIME=true electron-builder build --config ./electron-builder-runtime --publish never",
    "dir": "shx rm -rf src/dist && yarn build && electron-builder build --dir",
    "dir:with_runtime": "cross-env WITH_RUNTIME=true shx rm -rf src/dist && yarn build && electron-builder build --dir",
    "test": "jest",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "postinstall": "shx cp .erb/scripts/recast_fix.js node_modules/recast/parsers/babel.js && shx cp .erb/scripts/selector_fix.js node_modules/selector-generator/selector-generator.js",
    "postinstall_dll": "node -r @babel/register .erb/scripts/CheckNativeDep.js && electron-builder install-app-deps && yarn cross-env NODE_ENV=development webpack --config ./.erb/configs/webpack.dev.dll.babel.js && opencollective-postinstall && yarn-deduplicate yarn.lock",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zicjin/derealize.git"
  },
  "author": {
    "name": "Elliotreborn",
    "email": "elliotreborn@gmail.com",
    "url": "https://derealize.com"
  },
  "license": "private",
  "bugs": {
    "url": "https://github.com/zicjin/derealize/issues"
  },
  "homepage": "https://derealize.com",
  "jest": {
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/node_modules"
    ],
    "setupFiles": [
      "./.erb/scripts/CheckBuildsExist.js"
    ]
  },
